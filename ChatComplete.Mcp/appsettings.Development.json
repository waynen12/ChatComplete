{
  "McpServer": {
    "OpenTelemetry": {
      "EnableConsoleExporter": true,
      "EnablePrometheusExporter": true,
      "TraceSampleRate": 1.0
    },
    "Logging": {
      "LogLevel": "Debug",
      "EnableStructuredLogging": true
    }
  },
  
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "ChatComplete.Mcp": "Debug",
      "OpenTelemetry": "Information"
    }
  }
}